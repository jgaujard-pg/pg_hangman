{% extends 'base.html.twig' %}

{% block title %}
    {{ parent() }} {{ 'contact.page_title'|trans }}
{% endblock %}

{% block content %}
    <h2>{{ 'contact.title'|trans }}</h2>

    {{ form_start(contact_form) }}
    {{ form_rest(contact_form) }}
    <button type="submit" class="btn">{{ 'contact.button.send'|trans }}</button>
    {{ form_end(contact_form) }}
{% endblock %}
